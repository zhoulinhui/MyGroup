<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_regist"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroup_login"
    tools:context="com.my.zhou.group.RegistActivity">

    <include layout="@layout/include_login_item"></include>

    <LinearLayout
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginTop="90dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/include_regist_content"></include>

        <include layout="@layout/include_regist_button"></include>
    </LinearLayout>
</RelativeLayout>
